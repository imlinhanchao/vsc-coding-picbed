{
    "name": "coding-picture-bed",
    "displayName": "Coding Picture Bed",
    "description": "Auto Upload Picture to Coding Repository and generate markdown.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "coding-picbed.paste",
                "title": "Paste Picture"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "coding-picbed": "0.0.12"
    }
}
